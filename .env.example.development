KEYCLOAK_ISSUER=http://${KEYCLOAK_DOMAIN}:8080/realms/${KEYCLOAK_REALM}
NEXT_PUBLIC_API_BASE_URL=http://${APP_DOMAIN}:3000/api/trpc

LOG_LEVEL=debug