### This env file is for development purposes. It will be used by the CI.
NODE_ENV=development

POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=polysimdb

KEYCLOAK_DB_PASSWORD=secure_keycloak_db_password_that_is_not_needed_in_dev
KEYCLOAK_ADMIN_PASSWORD=secure_keycloak_admin_password_that_will_never_change_in_dev_but_should_in_prod
KEYCLOAK_REALM=polysim
KEYCLOAK_CLIENT_ID=polysim-app
